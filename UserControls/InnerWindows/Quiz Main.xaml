<UserControl x:Class="Quiz.UserControls.InnerWindows.Quiz_Main"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Quiz.UserControls.InnerWindows"
             mc:Ignorable="d" 
             d:DesignHeight="377.907" d:DesignWidth="349.125">
    <Grid>
        <Rectangle x:Name="RoundBorder" Width="Auto" Height="Auto" Fill="Black" RadiusX="40" RadiusY="40">

        </Rectangle>

        <Label x:Name="Title" Width="111" Height="Auto" Content="Quiz" Foreground="White" FontSize="50" Margin="119,0" HorizontalAlignment="Center" VerticalAlignment="Top"></Label>

        <!--Username Setting-->
        <TextBox x:Name="UsernameText" Text="Username" Margin="10,118,255,230" Background="{x:Null}" Foreground="White" FontSize="18" BorderBrush="{x:Null}"/>
        <TextBox x:Name="UsernameTextBox" Width="200" Height="30" Foreground="Black" Background="White" Margin="119,118,30,230" FontSize="18"></TextBox>
    </Grid>
</UserControl>
